{
  "languages": {
    "Nix": {
      "tab_size": 2,
      "language_servers": ["nixd"]
    }
  },
  "file_types": {
    "Kdl": ["*.kdl.tmpl"],
    "Shell Script": ["conf"]
  },
  "relative_line_numbers": "enabled",
  "agent": {
    "default_profile": "write",
    "model_parameters": []
  },
  "show_edit_predictions": false,
  "buffer_font_family": "CaskaydiaCove Nerd Font",
  "buffer_font_size": 16.0,
  "theme": "Wallust",
  "ui_font_family": "DejaVu Sans",
  "ui_font_size": 16.0,
  "vim_mode": true
}
